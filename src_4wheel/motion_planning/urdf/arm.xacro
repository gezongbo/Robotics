    <xacro:macro name="jaka_gazebo" >
        <xacro:macro name="arm_gazebo" params="link">
            <gazebo reference="${link}">
                <material>Gazebo/White</material>
                <mu1>0.61</mu1>
                <mu2>0.61</mu2>
            </gazebo>
        </xacro:macro>

