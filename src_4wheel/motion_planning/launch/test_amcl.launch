<launch>
    <!-- 运行rviz -->
    <!-- <node pkg="joint_state_publisher" name="joint_state_publisher" type="joint_state_publisher" />
    <node pkg="robot_state_publisher" name="robot_state_publisher" type="robot_state_publisher" /> -->
    <node pkg="rviz" type="rviz" name="rviz" args="-d $(find motion_planning)/config/nav_slam.rviz" />
    <!-- 加载地图 -->
    <arg name="map" default="myenv_nav.yaml" />
    <node name="map_server" pkg="map_server" type="map_server" args="$(find motion_planning)/map/$(arg map)"/>
    <!-- 启动AMCL节点 -->
    <include file="$(find motion_planning)/launch/nav_amcl.launch" />

</launch>
