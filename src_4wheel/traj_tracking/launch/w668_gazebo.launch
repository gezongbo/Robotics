<launch>

  <!-- specify the planning pipeline -->
  <arg name="pipeline" default="ompl" />

  <!-- Gazebo specific options -->
  <arg name="gazebo_gui" default="true"/>
  <arg name="paused" default="false"/>

  <!-- launch the gazebo simulator and spawn the robot -->
  <include file="$(find arm_moveit)/launch/gazebo.launch" >
    <arg name="paused" value="$(arg paused)"/>
    <arg name="gazebo_gui" value="$(arg gazebo_gui)"/>
    <arg name="world" value="my_empty.world"/>
    <arg name="robot" value="w668_nolaser.xacro"/>
  </include>

  <include file="$(find arm_moveit)/launch/demo.launch" pass_all_args="true">
    <!-- robot description is loaded by gazebo.launch, to enable Gazebo features -->
    <arg name="load_robot_description" value="false" />
    <arg name="moveit_controller_manager" value="ros_control" />
  </include>

  <!-- 启动控制节点 -->
  <node name="control" pkg="motion_planning" type="control" />
  <node name="pub_odom" pkg="motion_planning" type="pub_odom" />
  

  <!-- 初始化机械臂位置 -->
  <node name="init_arm" pkg="motion_planning" type="init_arm" />

  <!-- 包含导航launch文件 -->
  <!-- <include file="$(find motion_planning)/launch/nav_test.launch" /> -->

  <!-- 显示末端轨迹 -->
  <node name="show_path" pkg="traj_tracking" type="show_path" />

  <!-- rviz -->
  <node pkg="rviz" type="rviz" name="rviz_test" args="-d $(find traj_tracking)/config/show_path.rviz"/>
</launch>
