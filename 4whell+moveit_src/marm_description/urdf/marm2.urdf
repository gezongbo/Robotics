    <!-- hokuyo -->
    <!-- gazebo插件设置相关 -->
    
    <gazebo reference="wheel_1">
        <material>Gazebo/Red</material>
    </gazebo>
    <gazebo reference="wheel_2">
        <material>Gazebo/Black</material>
    </gazebo>
